[project]
title = Solitaire Klondike
version = 1.91
write_log = 0
compress_archive = 1
custom_resources = resources

[display]
width = 1080
height = 1440
high_dpi = 1
samples = 0
fullscreen = 0
update_frequency = 0
vsync = 0
display_profiles = /modules/render/custom.display_profilesc
dynamic_orientation = 0

[physics]
type = 2D
gravity_y = 0.0
debug = 0
debug_alpha = 1.0
world_count = 4
gravity_x = 0
gravity_z = 0
scale = 1
debug_scale = 1.0
max_collisions = 1
max_contacts = 1
contact_impulse_limit = 0
ray_cast_limit_2d = 64
ray_cast_limit_3d = 128
trigger_overlap_capacity = 16

[bootstrap]
main_collection = /collections/_loader/_loader.collectionc
render = /modules/render/custom.renderc

[graphics]
default_texture_min_filter = linear
default_texture_mag_filter = linear
max_draw_calls = 16
max_characters = 512
max_debug_vertices = 16
texture_profiles = /modules/render/custom.texture_profiles

[sound]
gain = 1
max_sound_data = 2
max_sound_buffers = 8
max_sound_sources = 4
max_sound_instances = 4

[resource]
http_cache = 0
max_resources = 128

[input]
repeat_delay = 0.5
repeat_interval = 0.2
gamepads = /builtins/input/default.gamepadsc
game_binding = /modules/input/game.input_bindingc
use_accelerometer = 0

[sprite]
max_count = 256
subpixels = 1

[spine]
max_count = 0

[model]
max_count = 0

[gui]
max_count = 4
max_particlefx_count = 0
max_particle_count = 0

[collection]
max_instances = 256

[collection_proxy]
max_count = 8

[collectionfactory]
max_count = 1

[factory]
max_count = 4

[ios]
pre_renderered_icons = 0
bundle_identifier = com.potatojam.classic.solitaire.klondike
infoplist = /builtins/manifests/ios/Info.plist
app_icon_120x120 = /art/other/icons/120.png
app_icon_180x180 = /art/other/icons/180.png
app_icon_76x76 = /art/other/icons/76.png
app_icon_152x152 = /art/other/icons/152.png
app_icon_57x57 = /art/other/icons/57.png
app_icon_114x114 = /art/other/icons/114.png
app_icon_72x72 = /art/other/icons/72.png
app_icon_144x144 = /art/other/icons/144.png
app_icon_167x167 = /art/other/icons/167.png
launch_image_1242x2208 = /art/other/preload/1242_2208.png
launch_image_2048x2732 = /art/other/preload/2048_2732.png
launch_image_1536x2048 = /art/other/preload/1536_2048.png
launch_image_768x1024 = /art/other/preload/768_1024.png
launch_image_750x1334 = /art/other/preload/750_1334.png
launch_image_640x1136 = /art/other/preload/640_1136.png
launch_image_640x960 = /art/other/preload/640_960.png
launch_image_320x480 = /art/other/preload/320_480.png
launch_image_1125x2436 = /art/other/preload/1125_2436.png

[android]
version_code = 1191
minimum_sdk_version = 16
target_sdk_version = 28
package = com.potatojam.classic.solitaire.klondike
manifest = /AndroidManifest.xml
iap_provider = GooglePlay
input_method = KeyEvent
immersive_mode = 1
debuggable = 0
app_icon_36x36 = /art/other/icons/36.png
app_icon_48x48 = /art/other/icons/48.png
app_icon_72x72 = /art/other/icons/72.png
app_icon_96x96 = /art/other/icons/96.png
app_icon_144x144 = /art/other/icons/144.png
app_icon_192x192 = /art/other/icons/192.png

[osx]
infoplist = /builtins/manifests/osx/Info.plist
bundle_identifier = example.unnamed

[windows]

[html5]
custom_heap_size = 0
heap_size = 256
htmlfile = /builtins/manifests/web/engine_template.html
cssfile = /builtins/manifests/web/light_theme.css
archive_location_prefix = archive
show_fullscreen_button = 1
show_made_with_defold = 1
scale_mode = downscale_fit
splash_image = /art/other/Title.png

[particle_fx]
max_count = 0
max_particle_count = 0

[iap]
auto_finish_transactions = 1

[network]
http_timeout = 0

[library]

[script]
shared_state = 1

[tracking]

[label]
max_count = 0
subpixels = 1

[profiler]
track_cpu = 0

[liveupdate]
settings = /liveupdate.settings

[tilemap]
max_count = 0
max_tile_count = 0

[native_extension]
app_manifest = /game_nophysics.appmanifest

