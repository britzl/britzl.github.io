[project]
title = Defold-Input
version = 0.1

[bootstrap]
main_collection = /examples/examples.collectionc
render = /builtins/render/default.renderc

[input]
game_binding = /in/bindings/all.input_bindingc
gamepads = /in/gamepads/in.gamepadsc

[display]
width = 1280
height = 720

[physics]
